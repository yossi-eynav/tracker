{
  "manifest_version": 2,
  "name": "extension",
  "version": "0.1",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
          "default_title": "extension"
  },
  "content_security_policy": "script-src 'self' http://localhost:3000 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "webRequest",
    "background",
    "*://*/*"
  ]
}